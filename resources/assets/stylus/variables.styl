$primary   = #08275e
$secondary = #424242
$accent    = #451e5f

$positive  = #4BBEAB
$negative  = #B05552
$info      = #35586A
$warning   = #D6B476

$light     = #bdbdbd
$dark      = #04091f
$faded     = #777

$input-text-color = $primary;
$input-label-color = $accent;

$generic-border-radius = 1px;
$body-font-size = 14px !default;

@import url('https://fonts.googleapis.com/css?family=Concert\+One&display=swap');
.title-page {
  font-family: 'Concert One', sans-serif, cursive;
  padding-bottom : 5px;
  border-bottom: 4px double $accent;
}


